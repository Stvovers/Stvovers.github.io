<mvc:View
	controllerName="sap.ui.demo.wt.controller.App"
	xmlns:l="sap.ui.layout"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true">
	<App>
		<pages>
			<Page title="{i18n>homePageTitle}">
				<content>
					<Panel
						headerText="{i18n>helloPanelTitle}">
						<content>
						
							<Label text="OTRS"/>
							<SearchField
								placeholder="10000000"
								width="25%"
								class="sapUiSmallMargin"
								search="onSearchOTRS"/>

								<Label text="EIM"/>
							<SearchField
								placeholder="6812"
								width="25%"
								class="sapUiSmallMargin"
								search="onSearchEIM"/>

						</content>
					</Panel>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>
